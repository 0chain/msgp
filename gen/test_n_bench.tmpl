func TestEncodeDecode{{.Name}}(t *testing.T) {
	t.Parallel()
	v := new({{.Name}})
	var buf bytes.Buffer
	n, _ := v.EncodeMsg(&buf)

	vn := new({{.Name}})
	nr, err := vn.DecodeMsg(&buf)
	if err != nil {
		t.Error(err)
	}

	if nr != n {
		t.Errorf("Wrote %d bytes; read %d bytes", n, nr)
	}

	buf.Reset()
	v.EncodeMsg(&buf)
	// Validate encoding by "skipping"
	// the entire object
	_, err = enc.NewDecoder(&buf).Skip()
	if err != nil {
		t.Error(err)
	}

	buf.Reset()
	v.EncodeMsg(&buf)

	ls := new({{.Name}})
	var left []byte 
	left, err = ls.UnmarshalMsg(buf.Bytes())
	if err != nil {
		t.Error(err)
	}
	if len(left) > 0 {
		t.Error("bytes left over...")
	}

	left, err = enc.Skip(buf.Bytes())
	if err != nil {
		t.Error(err)
	}
	if len(left) > 0 {
		t.Errorf("bytes left over after skip: %q", left)
	}
}

