func BenchmarkRead{{.Name}}(b *testing.B) {
	v := new({{.Name}})
	var buf bytes.Buffer
	v.EncodeMsg(&buf)
	rd := bytes.NewReader(buf.Bytes())
	dc := enc.NewDecoder(rd)
	b.ReportAllocs()
	b.ResetTimer()
	for i:=0; i<b.N; i++ {
		rd.Seek(0, 0)
		_, err := v.DecodeFrom(dc)
		if  err != nil {
			b.Fatal(err)
		}
	}
}

